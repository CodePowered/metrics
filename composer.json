{
    "type": "project",
    "license": "proprietary",
    "name": "codepowered/metrics",
    "description": "Implementation of consuming an API assignment.",
    "authors": [
        {
            "name": "Arturas Sungaila",
            "email": "codepowered@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.2",
        "symfony/property-info": "^5.2",
        "symfony/serializer": "^5.2",
        "symfony/yaml": "^5.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.2",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "sort-packages": true
    }
